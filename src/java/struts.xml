<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts  
Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">  

<struts>
    <constant name="struts.devMode" value="true" />	
    <package name="default" namespace="/" extends="struts-default">
        <action name="Home">
            <result>index.jsp</result>
        </action>
        <action name="Login">
            <result>login.jsp</result>
        </action>
        <action name="Logout">
            <result>logout.jsp</result>
        </action>
        <action name="ValidateCredentials">
            <result>ValidateCredentials.jsp</result>
        </action>
        <action name="saveRegUser">
            <result>saveRegUser.jsp</result>
        </action>
        <action name="ValidatedUsername">
            <result>UsernameExistValidate.jsp</result>
        </action>
         
         
         
         
        <action name="Rooms" >
            <result>/Rooms/frmRooms.jsp</result>
        </action>
        <action name="RoomsType" >
            <result>/Rooms/frmTypePrice.jsp</result>
        </action>
        <action name="RoomsList">
            <result>/Rooms/RoomList.jsp</result>
        </action>
        <action name="RoomListInnr">
            <result>/Rooms/RoomListInnr.jsp</result>
        </action>
        
        <action name="RoomInsert">
            <result>/Rooms/insertRoomDtls.jsp</result>
        </action>
        <action name="ValidateRoomNo">
            <result>/Rooms/ValidateRoomNo.jsp</result>
        </action>
        <action name="DeleteType">
            <result>/Rooms/DeleteType.jsp</result>
        </action>
        
        
        
        
        
        <action name="Guests">
            <result>/Guest/frmGuest.jsp</result>
        </action>
        <action name="InsertGuest">
            <result>/Guest/InsertGuest.jsp</result>
        </action>
        <action name="GuestList">
            <result>/Guest/GuestList.jsp</result>
        </action>
        <action name="GuestListInnr">
            <result>/Guest/GuestListInnr.jsp</result>
        </action>
        
        
    </package>
</struts>